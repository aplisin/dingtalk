{
    "name": "aplisin/dingtalk",
    "description": "It is probably the best SDK in the world for developing Dingtalk App",
    "keywords": [
        "dingtalk",
        "dingding",
        "dingtalk-sdk",
        "dingding-sdk",
        "sdk"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "IlhamTahir",
            "email": "yantaq@bilig.biz"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-SimpleXML": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-libxml": "*",
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.23",
        "symfony/http-foundation": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "easywechat-composer/easywechat-composer": "^1.0",
        "symfony/cache": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Aplisin\\DingTalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aplisin\\DingTalk\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "mockery/mockery": "^1.1"
    }
}
